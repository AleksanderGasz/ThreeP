{
    "ConnectionStrings": {
        "DefaultConnection": "DataSource=Data\\app.db;Cache=Shared"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Warning",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information",
            "Microsoft.AspNetCore.Components.Server.Circuits": "Warning"
        }
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information",
                "Microsoft.AspNetCore.Components.Server.Circuits": "Warning"
            }
        }
    },
"AllowedHosts": "*",
"DbSetup": true
}
